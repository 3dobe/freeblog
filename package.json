{
	"name": "freeblog",
	"description": "A lightweight blog framework",
	"version": "0.0.2",
	"engines": {
		"node": ">=0.8.0 <12.0.0"
	},
	"main": "app.js",
	"scripts": {
		"docker:mongo5": "docker run --rm -it -p 127.0.0.1:27017:27017 -v ~/docker-data/local-mongo5:/data/db --name mongo5 mongo:5",
		"docker:mongo4": "docker run --rm -it -p 127.0.0.1:27017:27017 -v ~/docker-data/local-mongo4:/data/db --name mongo4 mongo:4",
		"docker:mongo3": "docker run --rm -it -p 127.0.0.1:27017:27017 -v ~/docker-data/local-mongo3:/data/db --name mongo3 mongo:3",
		"docker:mongo2": "docker run --rm -it -p 127.0.0.1:27017:27017 -v ~/docker-data/local-mongo2:/data/db --name mongo2 mongo:2",
		"start": "node ."
	},
	"dependencies": {
		"underscore": "1.5.2",
		"async": "0.2.9",
		"express": "3.4.7",
		"fs-extra": "^0.24.0",
		"jade": "1.0.0",
		"node-uuid": "1.4.1",
		"mongoose": "4.13.21",
		"mongoose-auto-increment": "2.1.1"
	},
	"keywords": ["free", "blog", "cms"],
	"homepage": "https://github.com/3dobe/freeblog",
	"bugs": "https://github.com/3dobe/freeblog/issues",
	"repository": {
		"type": "git",
		"url": "git@github.com:3dobe/freeblog.git"
	},
	"license": "MIT"
}
